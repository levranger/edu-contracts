  "license": "ISC",
  "bugs": {
    "url": "https://github.com/levranger/edu-contracts/issues"
  },
  "homepage": "https://github.com/levranger/edu-contracts#readme",
  "devDependencies": {
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "prettier": "^2.7.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0"
  }
}
